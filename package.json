{
  "name": "md2overleaf",
  "version": "0.2.0",
  "description": "Obsidian plugin to export Markdown notes to Overleaf projects using Pandoc + Lua filters.",
  "main": "main.js",
  "scripts": {
    "dev": "esbuild main.ts --bundle --outfile=main.js --platform=node --external:obsidian --external:electron --watch",
    "build": "esbuild main.ts --bundle --outfile=main.js --platform=node --external:obsidian --external:electron"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "pandoc",
    "lua-filter",
    "latex",
    "overleaf",
    "export"
  ],
  "author": "Asaf Dayan",
  "license": "MIT",
  "dependencies": {
    "adm-zip": "^0.5.10",
    "fs-extra": "^11.2.0",
    "openurl": "^1.1.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "esbuild": "^0.19.0",
    "typescript": "^5.4.0"
  }
}
